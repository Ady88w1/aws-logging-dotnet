{
  "Serilog": {
    "Using": [ "AWS.Logger.SeriLog" ],
    "MinimumLevel": "Debug",
    "LogGroup": "AWSSeriLogGroup",
    "Region": "us-west-2",
    "LogStreamNameSuffix": "Custom",
    "LogStreamNamePrefix": "CustomPrefix",
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  }
}